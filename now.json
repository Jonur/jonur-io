{
  "version": 2,
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/styles.css$",
      "dest": "/styles.css"
    },
    {
      "src": "^/images/github.png$",
      "dest": "/images/github.png"
    },
    {
      "src": "^/images/ig.png$",
      "dest": "/images/ig.png"
    },
    {
      "src": "^/images/li.png$",
      "dest": "/images/li.png"
    },
    {
      "src": "^/images/so.png$",
      "dest": "/images/so.png"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
